{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "build:prod": {
      "dependsOn": ["^build:prod"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "typecheck": {
      "dependsOn": ["i18n", "^typecheck"]
    },
    "i18n": {
      "inputs": [
        "src/i18n/*/index.ts",
        "src/i18n/formatters.ts",
        ".typesafe-i18n.json"
      ],
      "outputs": [
        "src/i18n/i18n-types.ts",
        "src/i18n/i18n-util.ts",
        "src/i18n/i18n-util.*.ts",
        "src/i18n/i18n-react.tsx"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["typecheck"]
    },
    "//#check": {},
    "//#check:fix": {
      "cache": false
    }
  },
  "remoteCache": {
    "enabled": true,
    "teamId": "l-etabli"
  }
}
