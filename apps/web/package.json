{
  "name": "@tasks/web",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:with-i18n": "vite dev & typesafe-i18n",
    "start": "node .output/server/index.mjs",
    "build": "vite build && pnpm copy:migrations",
    "copy:migrations": "mkdir -p .output/server/migrations && cp ../../packages/db/dist/src/migrations/*.js .output/server/migrations/ && cp ../../packages/db/dist/src/runDbMigrations.js .output/server/runDbMigrations.mjs && cp ../../packages/db/dist/src/connection.js .output/server/",
    "build:prod": "vite build",
    "serve": "vite preview",
    "test": "echo \"No tests\" #vitest run",
    "typecheck": "tsc --noEmit",
    "check:fix": "pnpm -w check:fix",
    "fullcheck": "pnpm -w fullcheck",
    "db:up": "dotenv -e .env.local -- pnpm --dir ../../packages/db db:up",
    "db:create": "pnpm --dir ../../packages/db db:create",
    "typesafe-i18n": "typesafe-i18n"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.23",
    "@ai-sdk/react": "^2.0.60",
    "@modelcontextprotocol/sdk": "^1.19.1",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-form": "^1.23.6",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.132.47",
    "@tanstack/react-router-devtools": "^1.132.47",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.132.48",
    "@tanstack/react-store": "^0.7.7",
    "@tanstack/router-plugin": "^1.132.47",
    "@tanstack/store": "^0.7.7",
    "@tasks/core": "workspace:*",
    "@tasks/db": "workspace:*",
    "@tasks/sentry": "workspace:*",
    "@trpc/client": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@trpc/tanstack-react-query": "^11.6.0",
    "ai": "^5.0.60",
    "better-auth": "^1.3.27",
    "highlight.js": "^11.11.1",
    "kysely": "^0.28.7",
    "lucide": "^0.545.0",
    "lucide-react": "^0.545.0",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.1.14",
    "typesafe-i18n": "^5.26.2",
    "vinxi": "^0.5.8",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv-cli": "^10.0.0",
    "jsdom": "^27.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}